<?xml version="1.0" encoding="UTF-8"?>
<T3DataStructure>
	<ROOT>
		<el>
		<settings.templateFile>
				<TCEforms>
					<label>Template File (optional)</label>
					<config>
						<type>input</type>
						<wizards type="array">
						<_PADDING type="integer">2</_PADDING>
							<link type="array">
								<type>popup</type>
								<title>Link</title>
								<icon>link_popup.gif</icon>
								<script>browse_links.php?mode=wizard&amp;act=file</script>
								<JSopenParams>height=300,width=500,status=0,menubar=0,scrollbars=1</JSopenParams>
								<params type="array">
									<blindLinkOptions>page,url,mail,spec,folder</blindLinkOptions>
									<allowedExtensions>html</allowedExtensions>
								</params>

							</link>
						</wizards>
					</config>
				</TCEforms>
			</settings.templateFile>
		<settings.zoom>
			<TCEforms>
				<label>Zoom</label>
				<config>
					<type>input</type>
					<default>4</default>
					<eval>num</eval>
					<size>2</size>
				</config>
			</TCEforms>
		</settings.zoom>
		<settings.latitude>
			<TCEforms>
				<label>Latitude</label>
				<config>
					<type>input</type>
					<default>0</default>
				</config>
			</TCEforms>
		</settings.latitude>
		<settings.longitude>
			<TCEforms>
				<label>Longitude</label>
				<config>
					<type>input</type>
					<default>0</default>
				</config>
			</TCEforms>
		</settings.longitude>
		<settings.height>
			<TCEforms>
				<label>Height</label>
				<config>
					<type>input</type>
					<default>300</default>
					<eval>num</eval>
					<size>2</size>
				</config>
			</TCEforms>
		</settings.height>
		<settings.kml>
			<TCEforms>
				<label>Fichero KML</label>
				<config>
					<type>input</type>
					<wizards type="array">
					<_PADDING type="integer">2</_PADDING>
						<link type="array">
							<type>popup</type>
							<title>Link</title>
							<icon>link_popup.gif</icon>
							<script>browse_links.php?mode=wizard&amp;act=file</script>
							<JSopenParams>height=300,width=500,status=0,menubar=0,scrollbars=1</JSopenParams>
							<params type="array">
								<blindLinkOptions>page,url,mail,spec,folder</blindLinkOptions>
								<allowedExtensions>kml</allowedExtensions>
							</params>

						</link>
					</wizards>
				</config>
			</TCEforms>
		</settings.kml>
		</el>
	</ROOT>
</T3DataStructure>

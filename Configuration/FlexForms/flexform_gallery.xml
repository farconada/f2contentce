<?xml version="1.0" encoding="UTF-8"?>
<T3DataStructure>
	<ROOT>
		<type>array</type>
		<el>
			<settings.templateFile>
				<TCEforms>
					<label>Template File (optional)</label>
					<config>
						<type>input</type>
						<wizards type="array">
						<_PADDING type="integer">2</_PADDING>
							<link type="array">
								<type>popup</type>
								<title>Link</title>
								<icon>link_popup.gif</icon>
								<script>browse_links.php?mode=wizard&amp;act=file</script>
								<JSopenParams>height=300,width=500,status=0,menubar=0,scrollbars=1</JSopenParams>
								<params type="array">
									<blindLinkOptions>page,url,mail,spec,folder</blindLinkOptions>
									<allowedExtensions>html</allowedExtensions>
								</params>

							</link>
						</wizards>
					</config>
				</TCEforms>
			</settings.templateFile>
			<settings.displayTime>
				<TCEforms>
					<label>Display time in seconds</label>
					<config>
						<type>input</type>
						<size>6</size>
						<default>3</default>
					</config>
				</TCEforms>
			</settings.displayTime>
			<settings.minWidth>
				<TCEforms>
					<label>Min Width</label>
					<config>
						<type>input</type>
						<size>6</size>
						<default>100</default>
					</config>
				</TCEforms>
			</settings.minWidth>
			<settings.maxWidth>
				<TCEforms>
					<label>Max Width</label>
					<config>
						<type>input</type>
						<size>6</size>
						<default>400</default>
					</config>
				</TCEforms>
			</settings.maxWidth>
			<settings.images>
				<section>1</section>
				<type>array</type>
				<el>
					<image>
						<type>array</type>
						<tx_templavoila>
							<title>Image</title>
						</tx_templavoila>
						<el>
							<file>
								<TCEforms>
									<label>Image File</label>
									<config>
										<type>group</type>
										<internal_type>file</internal_type>
										<allowed>gif,jpg,jpeg,tif,bmp,pcx,tga,png,pdf,ai</allowed>
										<max_size>5000</max_size>
										<uploadfolder>uploads/f2contentce/cyclegallery</uploadfolder>
										<maxitems>1</maxitems>
										<size>1</size>
										<selectedListStyle>width:200px</selectedListStyle>
									</config>
								</TCEforms>
							</file>
							<alt>
								<TCEforms>
									<label>Alt text</label>
									<config>
										<type>input</type>
										<size>50</size>
										<default></default>
									</config>
								</TCEforms>
							</alt>
						</el>
					</image>
				</el>
			</settings.images>
		</el>
	</ROOT>
</T3DataStructure>